{
  "manifest_version": 2,
  "name": "Urgent on Notification",
  "version": "1.0.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [{
    "all_frames": true,
    "js": ["content.js"],
    "matches": ["<all_urls>"],
    "run_at": "document_end"
  }],
  "web_accessible_resources": ["content.js", "page.js"],
  "description": "Set window urgent hint on web notification."
}
